{
  "name": "influscraper",
  "version": "1.0.0",
  "description": "> In development. See docs/dev_plan.md for progress.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "start": "bun run dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:concurrent": "vitest src/tests/concurrent.test.ts",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality": "bun run type-check && bun run lint && bun run format:check",
    "fix-all": "bun run format && bun run lint:fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.1.0",
    "dotenv": "^16.5.0",
    "hono": "^4.7.11",
    "ioredis": "^5.6.1",
    "p-queue": "^8.1.0",
    "pino": "^9.7.0",
    "playwright": "^1.53.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bun": "latest",
    "@types/cheerio": "^1.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/p-queue": "^3.2.1",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "axios": "^1.9.0",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  }
}
